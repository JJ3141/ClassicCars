@model IEnumerable<User>

<h2>Users</h2>

<a asp-action="Create">Create New User</a>

<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Username</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.Username</td>
                <td>@user.Password</td>
            </tr>
        }
    </tbody>
</table>
